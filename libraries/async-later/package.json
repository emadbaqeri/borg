{
  "name": "async-later",
  "version": "0.1.7",
  "description": "Functional directives for Promise and AsyncIterable",
  "types": "src/index.ts",
  "main": "./dist/node/cjs.js",
  "module": "./dist/node/esm.js",
  "browser": "./dist/browser/esm.js",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "test:node": "tape dist/node/test.cjs | tap-spec; nyc report ---reporter=text",
    "test:web": "rushx build && cat dist/browser/test.js | testling -u",
    "lint": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/functionland/fula/tree/main/libraries/async-later"
  },
  "keywords": [
    "promise",
    "async-iterable",
    "stream",
    "observable",
    "fp",
    "functional",
    "functional-programming"
  ],
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.14.8",
    "streaming-iterables": "^6.0.0",
    "tap-spec": "^5.0.0",
    "ts-node": "^10.4.0",
    "nyc": "^15.1.0",
    "nodemon": "^2.0.7",
    "tape": "~5.3.2",
    "@types/tape": "^4.13.2",
    "browserify": "^17.0.0",
    "testling": "^1.7.3",
    "typescript": "^4.3.5",
    "esbuild-plugin-babel": "^0.2.3",
    "@babel/core": "~7.15.5",
    "@babel/preset-typescript": "~7.15.0",
    "@babel/preset-env": "~7.15.6",
    "eslint": "~8.4.1",
    "@typescript-eslint/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "~5.6.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.2"
  },
  "exports": {
    ".": {
      "browser": {
        "require": "./dist/browser/cjs.cjs",
        "import": "./dist/browser/esm.js"
      },
      "node": {
        "require":"./dist/node/cjs.cjs",
        "import": "./dist/node/esm.js"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "~7.15.4"
  }
}
